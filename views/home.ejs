<%- include("partials/head.ejs") %>
<body>
  
  <%- include("partials/navbar.ejs") %>
    <div class="container">
      <h1 class="mb-3">Perguntas</h1>
      <a href="/ask" class="askColor btnAsk mb-5">Perguntar</a>
      <% asks.forEach(ask => { %>
        <div class="card mb-3">
          <div class="card-body">
            <h3><%- ask.title %></h3>
          </div>
          <div class="card-footer">
            <button class="btnAsk">Responder</button>
          </div>
        </div>
      <% }) %>
    </div>
</body>
<%- include("partials/footer.ejs") %>